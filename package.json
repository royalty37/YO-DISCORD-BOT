    "@distube/spotify": "^1.5.1",
    "@distube/yt-dlp": "^1.1.3",
    "@types/node-schedule": "^2.1.0",
    "dayjs": "^1.11.7",
    "discord.js": "^14.7.1",
    "distube": "^4.0.4",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "ffmpeg-static": "^5.1.0",
    "mongodb": "^4.13.0",
    "node-schedule": "^2.1.1",
    "openai": "^3.1.0",
    "sodium": "^3.0.2",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@swc/core": "^1.3.30",
    "@types/node": "^18.11.17",
    "eslint": "^8.30.0",
    "nodemon": "^2.0.20",
