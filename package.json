{
  "name": "yo-discord-bot",
  "version": "1.0.0",
  "description": "Personal Discord bot for YOZA Discord channel",
  "main": "index.js",
  "exports": "./build/index.js",
  "scripts": {
    "start": "npm run build && env-cmd -e prod node build/index.js",
    "deploy-commands": "npm run build && node build/deploy-commands.js",
    "deploy-start": "npm run deploy-commands && node build/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf ./build && tsc",
    "dev": "env-cmd -e prod,dev nodemon src/index.ts"
  },
